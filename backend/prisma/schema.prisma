generator client {
  provider      = "prisma-client-js"
  // Ensure Lambda (Amazon Linux, OpenSSL 3) has the correct engine
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Item {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
}


